datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

generator photon {
  provider = "photonjs"
}

model Genre {
  id   String @default(cuid()) @id
  name String @unique
}

model Artist {
  id           String  @default(cuid()) @id
  name         String
  album        Album[]
}

model Album {
  id          String   @default(cuid()) @id
  name        String
  releaseYear Int
  rating      Int
  artist      Artist[]
  genre       Genre?
}

model Playlist {
  id     String  @default(cuid()) @id
  name   String
  public Boolean
  album  Album[]
}